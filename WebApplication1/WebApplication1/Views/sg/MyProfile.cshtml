
@{
    ViewBag.Title = "MyProfile";
    Layout = "~/Views/sgLayoutPage1.cshtml";
}

<div class="col-md-8 order-md-1 validform2">
    <form action="/sg/MyProfile" method="post">
        <h4>Chnage Password</h4>
        <div class="form-group">
            <label>Old Password</label>
            <input type="password" class="form-control" placeholder="Enter old Password" required="" name="password1">
        </div>

        <div class="form-group">
            <label>New Password</label>
            <input type="password" class="form-control" placeholder="Enter New Password" required="" name="password2">
        </div>

        <div class="form-group">
            <label>Re-Enter New Password</label>
            <input type="password" class="form-control" placeholder="Re-Enter New Password" required="" name="password">
        </div>
        @*<button class="btn btn-primary btn-lg btn-block error-w3l-btn">Submit</button>*@
        <input type="submit" name="submit" value="Submit" class="btn btn-primary btn-lg btn-block error-w3l-btn" />
    </form>
</div>

@if (TempData["msg"] != null)
{
    <script type="text/javascript">
        alert('@TempData["msg"]');
        window.location.href = "@Url.Action("slogin","sg")";

    </script>
}

@if (TempData["msg1"] != null)
{
    <script type="text/javascript">
        alert('@TempData["msg1"]');
        window.location.href = "@Url.Action("MyProfile","sg")";

    </script>
}

 




